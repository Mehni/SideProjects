  <ThingDef ParentName="TM_D_Catfish">
    <defName>D_CC_Catfish</defName>
    <label>Catfish</label>
    <!-- <description>D grade Catfish. And length is 60cm and weight is 3kg. Compare to other same grade Catfish, It is average weight and average length.</description> -->
    <statBases>
      <Mass>3.2</Mass>
      <Nutrition>0.96</Nutrition>
      <MarketValue>30.6</MarketValue>
    </statBases>
    <butcherProducts>
      <Meat_Catfish>24</Meat_Catfish>
    </butcherProducts>
    <comps>
      <li Class = "FishGeneration.CompProperties_FishProps">
        <grade>D</grade>
      </li>
    </comps>
    <modExtensions>
      <li Class="TOBE_Fishing.FishProperties">
        <grade>D</grade>
        <biomes>
          <TemperateForest>30</TemperateForest>
