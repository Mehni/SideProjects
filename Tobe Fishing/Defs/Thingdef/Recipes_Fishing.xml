<?xml version="1.0" encoding="utf-8" ?>
<Defs>


	<RecipeDef>
		<defName>Fishing_fast_Fish</defName>
		<label>Fishing fish fast.</label>
		<description>Fishing a fish fast. Lower chance to catch a fish than normal</description>
		<jobString>Fishing.</jobString>
		<workAmount>1800</workAmount>
		<workSpeedStat>UnskilledLaborSpeed</workSpeedStat>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Paste_bait_Category</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Paste_bait_common</li>
			</thingDefs>
			<specialFiltersToDisallow>
				<li>AllowRotten</li>
			</specialFiltersToDisallow>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<thingDefs>
				<li>Paste_bait_common</li>
			</thingDefs>
		</defaultIngredientFilter>
		<recipeUsers>
			<li>TOBE_FishingSpot</li>
		</recipeUsers>
		<modExtensions>
			<li Class="TOBE_Fishing.RecipeOptions">
				<successChance>0.5</successChance>
				<!-- from 0 to 1, where 1 is 100% -->
				<increasedFishQuality>-1</increasedFishQuality>
				<!-- should be less than amount of fish qualities. Can be negative too. -->
			</li>
		</modExtensions>
	</RecipeDef>

